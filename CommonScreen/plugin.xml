<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
		<plugin>

            <!-- the plugin name [A-Za-z_] -->
            <pluginName>CommonScreen</pluginName>

            <!-- a short description -->
			<pluginDescription><![CDATA[]]></pluginDescription>

            <!-- meta data -->
            <pluginVersion>1.0</pluginVersion>
            <pluginRevision>1</pluginRevision>
            <pluginBuildTimestamp>0</pluginBuildTimestamp>

            <!-- put in the URL for the plugin configuration eg. /plugin/WorkFlow/admin/index -->
            <pluginIframeSrc></pluginIframeSrc>

            <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
			<pluginClassName>\CommonScreen\Plugin</pluginClassName>

            <!-- include paths relative to plugin-directory -->
			<pluginIncludePaths>
				<path>/CommonScreen/lib</path>
                                <path>/CommonScreen/models</path>
			</pluginIncludePaths>

			<!-- namespaces to register with autoloader-->
			<pluginNamespaces>
				<namespace>CommonScreen</namespace>
			</pluginNamespaces>

			<!-- js files with path relative to plugin-directory -->
			<pluginJsPaths-extjs6>
                            <path>/plugins/CommonScreen/static6/js/workflow.js</path>
                            <path>/plugins/CommonScreen/static6/js/screen.js</path>
                            <path>/plugins/CommonScreen/static6/js/overrideDefaults.js</path>
			</pluginJsPaths-extjs6>

			<!-- css files with path relative to plugin-directory -->
			<pluginCssPaths-extjs6>
                <path>/plugins/CommonScreen/static6/css/style.css</path>
			</pluginCssPaths-extjs6>
		</plugin>
</zend-config>
